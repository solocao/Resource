	<!--Main-->
	<section class="container">
		<div class="registerArea">
			<div class="row">
				<div class="col-md-8 Content">
					<h4>
						<strong>账号注册-基本信息</strong>
					</h4>
					<form class="form-horizontal" role="form" method="post"
						id="regform">
						<div class="form-group">
							<label for="inputuser" class="col-sm-2 control-label">用户名:<i>*</i></label>
							<div class="col-sm-10">
								<input type="text" class="form-control" name="c_name" id="c_name"
									validate='[{"type":"notNull","msg":"用户名不能为空！"},{"type":"regex","reg":"^[a-zA-Z]{1}[a-zA-Z0-9]{2,9}$","msg":"用户名必须是字母开头并且不能少于3位不能多余10位"}]' />
							</div>
						</div>
						<div class="form-group">
							<label for="inputuser" class="col-sm-2 control-label">邮箱地址:<i>*</i></label>
							<div class="col-sm-10">
								<input type="text" name="email" class="form-control" id="email"
									validate='[{"type":"regex","reg":"^(\\w)+(\\.\\w+)*@(\\w)+((\\.\\w+)+)$","msg":"邮箱地址不符合规范!"}]' />
							</div>
						</div>
						<div class="form-group">
							<label for="inputuser" class="col-sm-2 control-label">密码:<i>*</i></label>
							<div class="col-sm-10">
								<input type="password" name="password" class="form-control" id="password"
									validate='[{"type":"notNull","msg":"密码不能为空！"}]' id="password" />
							</div>
						</div>
						<div class="form-group">
							<label for="inputuser" class="col-sm-2 control-label">确认密码:<i>*</i></label>
							<div class="col-sm-10">
								<input type="password" class="form-control"
									validate='[{"type":"notNull","msg":"确认密码不能为空！"},{"type":"equalTo","target":"#password","msg":"确认密码不一致"}]'
									name="confirmPassword" id="confirmPassword" />
							</div>
						</div>
						<div class="form-group">
							<div class="col-sm-offset-2 col-sm-10">
								<div class="checkbox">
									<label> <input type="checkbox"> 我同意《服务协议》
									</label>
								</div>
							</div>
						</div>
						<div class="form-group">
							<div class="col-sm-offset-2 col-sm-10">
								<button type="button" class="btn btn-block btn-info"
									onclick="regist()">注册</button>
							</div>
						</div>
					</form>
				</div>
				<div class="col-md-4 Link">
					<h4>已注册？现在就登录</h4>
					<img src="img/regist/reg_img.jpg" />
				</div>
			</div>
		</div>

	</section>

	<footer class="navbar-inverse"></footer>

	<script type="text/javascript" src="/p2p_home/js/jquery.1.9.min.js"></script>
	<script type="text/javascript" src="/p2p_home/js/bootstrap.min.js"></script>
	<script type="text/javascript" src="/p2p_home/js/customer.js"></script>
	<script type="text/javascript" src="/p2p_home/js/jquery.validate.js"></script>
	<script type="text/javascript">
		$(function() {
			$("#regform").validate();
		});
	</script>
</body>

